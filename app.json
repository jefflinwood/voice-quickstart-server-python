{
  "name": "Twilio Mobile Quickstart",
  "description": "A simple Python server to distribute access tokens and route calls for your mobile app",
  "repository": "https://github.com/jefflinwood/voice-quickstart-server-python",
  "keywords": [
    "twilio",
    "voice",
    "mobile"
  ],
  "env": {
    "ACCOUNT_SID": {
      "description": "Your Twilio Account Sid",
      "required": true
    },
    "API_KEY": {
      "description": "Your Twilio API Key",
      "required": true
    },
    "API_KEY_SECRET": {
      "description": "Your Twilio API Key Secret",
      "required": true
    },
    "GCM_CREDENTIAL_SID": {
      "description": "Your Twilio GCM Credential Sid",
      "required": false
    },
    "APNS_CREDENTIAL_SID": {
      "description": "Your Twilio APNS Credential Sid",
      "required": false
    },
    "APP_SID": {
      "description": "Your TwiML App Sid that points to this web server",
      "required": true
    }
  }
}
